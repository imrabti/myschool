<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with type="com.gsr.myschool.back.client.resource.message.MessageBundle" field="labelText"/>
    <g:HTMLPanel >
        <g:Label text="{labelText.labelsName}" width="134px" ui:field="nameLabel"/>
        <g:TextBox ui:field="name"/>
        <g:Label text="{labelText.labelsRegex}" width="134px" ui:field="regexLabel"/>
        <g:ValueListBox ui:field="regex"/>
        <g:Label text="{labelText.labelsParent}" width="134px" ui:field="parentLabel"/>
        <g:ValueListBox ui:field="parent"/>
        <g:Label text="{labelText.labelsSystemDefLov}" ui:field="systemDefLovLabel"/>
        <g:CheckBox ui:field="systemDefLov"/>
        <g:Button text="{labelText.buttonsSave}" ui:field="save"/>
    </g:HTMLPanel>
</ui:UiBinder>