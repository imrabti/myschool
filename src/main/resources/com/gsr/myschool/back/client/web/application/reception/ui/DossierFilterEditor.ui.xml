<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:boot='urn:import:com.github.gwtbootstrap.client.ui'
             xmlns:b2='urn:import:com.github.gwtbootstrap.datepicker.client.ui'>
    <g:HTMLPanel>
        <boot:Form type="HORIZONTAL">
            <boot:ControlGroup>
                <boot:ControlLabel>N° Dossier</boot:ControlLabel>
                <boot:Controls>
                    <boot:TextBox ui:field="numDossier" width="250px"/>
                </boot:Controls>
            </boot:ControlGroup>

            <boot:ControlGroup>
                <boot:ControlLabel>Nom ou prénom</boot:ControlLabel>
                <boot:Controls>
                    <boot:TextBox ui:field="firstnameOrlastname" width="250px"/>
                </boot:Controls>
            </boot:ControlGroup>
        </boot:Form>
    </g:HTMLPanel>
</ui:UiBinder>
