<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:boot='urn:import:com.github.gwtbootstrap.client.ui'>
    <ui:style>
        .menuWrapper {
            padding: 20px 20px;
            height: 100%;
        }

        .menu {
            min-height: 200px;
        }
    </ui:style>

    <g:HTMLPanel styleName="{style.menuWrapper}">
        <boot:WellNavList addStyleNames="{style.menu}">
            <boot:NavHeader>Menu</boot:NavHeader>
            <boot:NavLink ui:field="preInscription" icon="CLOUD">Pré-inscriptions</boot:NavLink>
            <boot:NavLink ui:field="reception" icon="FOLDER_OPEN">Reception des dossiers</boot:NavLink>
            <boot:NavLink ui:field="validation" icon="CHECK">Validation des dossiers</boot:NavLink>
            <boot:NavLink ui:field="userPortal" icon="USER">Utilisateurs Portail</boot:NavLink>
            <boot:NavLink ui:field="userGsr" icon="SUITCASE">Utilisateurs GSR</boot:NavLink>
            <boot:NavHeader>Paramètrage</boot:NavHeader>
            <boot:NavLink ui:field="genSettings" icon="UMBRELLA">Configuration Globale</boot:NavLink>
            <boot:NavLink ui:field="valueList" icon="TASKS">Liste de valeurs</boot:NavLink>
        </boot:WellNavList>
    </g:HTMLPanel>
</ui:UiBinder>
